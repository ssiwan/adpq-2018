{
  "name": "adpq2018-server",
  "version": "0.0.1",
  "description": "Homeowners Toolbox Inc. DBA HOTB Software submission for the California Department of Technology ADPQ in 2018",
  "keywords": [
    "ADPQ",
    "2018"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HOTB-Software/adpq-2018"
  },
  "main": "./src/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "build": "",
    "clean": "",
    "lint": "./node_modules/.bin/eslint . --fix",
    "test": "",
    "docker:start": "npm run docker:build && docker run -e NODE_ENV=local -d --restart=always -p 3001:3001 --name api -i api && clear",
    "docker:build": "docker build --force-rm --no-cache -t api . && clear",
    "docker:stop": "docker stop api && clear",
    "docker:teardown": "npm run docker:stop && docker rm api && docker rmi api -f && clear"
  },
  "dependencies": {
    "aws-sdk": "2.202.0",
    "bcrypt": "1.0.3",
    "body-parser": "1.16.0",
    "cors": "2.8.4",
    "express": "4.16.2",
    "express-brute": "1.0.1",
    "express-brute-mongoose": "1.0.2",
    "express-healthcheck": "0.1.0",
    "express-winston": "2.5.0",
    "helmet": "3.10.0",
    "jsonwebtoken": "8.1.1",
    "mongoose": "5.0.3",
    "sendmail": "1.2.0",
    "winston": "2.4.0",
    "winston-mongodb": "3.0.1"
  },
  "devDependencies": {
    "eslint": "^4.17.0"
  },
  "engines": {
    "node": "9.5.0",
    "npm": "5.5.1"
  },
  "os": [
    "any"
  ],
  "private": false,
